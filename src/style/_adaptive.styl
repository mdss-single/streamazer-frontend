/*  <768px>  */
@media screen and (min-width:768px)
	.page-title
		overflow hidden
		span
			position relative
			&:before
			&:after
				position absolute
				top 50%
				width calc(1000% - 30px)
				height 9px
				border solid #303030
				border-width 2px 0
				content ''
				transform translateY(-50%)
			&:before
				left -1000%
			&:after
				right -1000%
	.menu
		width 400px
		&__logo
			padding 14px 130px 0 50px
		&__close
			top 26px
			right 26px
			width 26px
			height 26px
			&:before
			&:after
				height 2px
		&__nav
			padding 10px 20px 10px 50px
		&__lang
			margin 20px 20px 0 50px
	.header
		height 163px
		&--home
			height 661px
		^[0]&--home
			&:before
				background-image url('../i/header-planets-768-home.png')
				background-size 100% auto
			&:after
				height 73px
				background-position 210px 0
			& ^[0]__bg
				&:before
					top 40px
					left 50%
					background-image url('../i/header-star.png')
				&:after
					position absolute
					right 15px
					bottom 14px
					z-index 3
					width 178px
					height 159px
					background url('../i/header-guy.png') no-repeat 0 0 / 100% auto
					content ''
			& ^[0]__lang
				display block
				top 50px
				right 176px
				left auto
				width 45px
				height auto
				line-height 1
				background none
				border 0
				box-shadow none
				&--active
					.header__lang-current
						color #fff
						&:after
							position absolute
							bottom 0
							right 0
							left 16px
							height 1px
							background-color #dba601
							content ''
				&-list
					margin-top 6px
				&-item
					&:hover
						color #fff
						text-decoration none
						&:after
							background-color #dba601
					&:after
						position absolute
						bottom 0
						right 0
						left 16px
						height 1px
						background-color #7d5e01
						content ''
		&__slogan
			&--home
				display block
				position absolute
				top 215px
				right 15px
				left 15px
				color #fff
				font-weight 500
				font-size 6.2rem
				font-family fontFira
				text-align justify
				margin 0
				& ^[1]--ready
					display none
				& ^[1]--today
					font-weight 400
				& ^[1]--shadow
					position relative
					color transparent
					&:before
						position absolute
						top 0
						left 0
						z-index 5
						overflow hidden
						height 59%
						color #fff
						content attr(data-slogan)
						pointer-events none
					&:after
						position absolute
						top 0
						left 0
						z-index 4
						height 100%
						color #dbdbdb
						content attr(data-slogan)
						pointer-events none
		&__description
			&--home
				top 280px
				color #afafaf
				font-size 3.4rem
		&:before
			background url('../i/header-planets-768.png') no-repeat 0 0 / cover
		&:after
			height 40px
		&__bg
			position absolute
			top 0
			right 0
			bottom 0
			left 0
			&:before
				position absolute
				top -50px
				left calc(50% + 50px)
				z-index 3
				width 290px
				height 249px
				background-image url('../i/header-star.png')
				content ''
				transform translateX(-50%)
		&__logo
			top 26px
			right auto
			left 15px
		&__menu
			top 36px
		&__lang
			display none
		&__register-link
			display none
			&--home
				display block
				top 410px
				right auto
				left 50%
				width 286px
				transform translateX(-50%)
				&:before
					top -47px
					width 1px
					height 30px
	.footer
		&__logo
			display block
			position absolute
			top 47px
			left 15px
		&__copyright
			display block
			position absolute
			top 156px
			left 15px
			color #7e7e7e
			font-size 1.6rem
			line-height 2.4rem
			p
				margin 0
		&__social
			top 44px
			left 390px
			text-align left
			transform none
		&__payment
			top 140px
			left 390px
			width 348px
			transform none
			&-title
				text-align left
				margin 0 0 20px
			&-item
				margin-bottom 0
	.pricing
		&__list
			margin-top 45px
			.flickity-prev-next-button
				top -102px
				&.previous
					right 30px
					left auto
				&.next
					right 0
					left auto
		&__item
			width calc(50% - 15px)
			margin-right 30px
			&:last-child
				margin-right 0
	.home-pricing__all
		color #a2a1a1
		text-decoration none
		span
			position relative
			padding-bottom 4px
			&:before
				position absolute
				right 0
				bottom 0
				left 0
				height 1px
				background-color #907318
				content ''
				transition all .2s
			&:hover
				&:before
					background-color #a2a1a1
	.home-pricing
		.page-title
			&:after
				position absolute
				top 0
				right 0
				bottom 0
				width 64px
				background-color #353535
				content ''
	.about-us
		.page-title
			span
				&:before
				&:after
					border-color #353535
	.why-we
		.page-title
			span
				&:before
				&:after
					border-color #3b3b3b
		&__items
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			flex-wrap wrap
			justify-content space-between
			margin-top -20px
		&__item
			width 48%
			margin-top 55px
	.articles
		&__items
			column-count 2
			column-gap 30px
		&__loadmore
			.btn
				width auto
				padding 0 55px
	.article
		&__item
			position relative
			background-color #5f5f5f
			border solid #696969
			border-width 5px 5px 0
			margin-bottom 30px
			box-shadow 0 5px 10px rgba(#000, .19)
			-webkit-column-break-inside avoid
			page-break-inside avoid
			break-inside avoid
			&-name
				padding 0 30px
			&-info
				padding 0 30px
			&-readmore
				position relative
				text-align left
				background-color #696969
				padding 16px 30px 21px
				&:before
					position absolute
					right -5px
					bottom 0
					left -5px
					height 5px
					background-color #d9a501
					content ''
				a
					width auto
					height 49px
					line-height 45px
					border-radius (@height / 2)
					padding 0 26px
					box-shadow 0 4px 2px rgba(#2f2f2f, .28)
					&:hover
						color #000
						text-decoration none
						background-color #fbc10a
						border-color @background-color
						box-shadow 0 4px 2px rgba(#684e00, .42)
	.blog
		margin-top 30px
		&__item
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			margin-bottom 50px
			&-pic
				width 228px
			&-main
				flex 1
				margin-left 30px
			&-title
				margin -8px 0 20px
			&-info
				margin-bottom 20px
			&-text
				display block
				color #e0e0e0
				font-size 1.6rem
				line-height 2.4rem
				margin-bottom 20px
			&-readmore
				text-align left
				a
					width auto
					height 49px
					line-height 45px
					border-radius (@height / 2)
					padding 0 26px
					box-shadow 0 4px 2px rgba(#2f2f2f, .28)
					&:hover
						color #000
						text-decoration none
						background-color #fbc10a
						border-color @background-color
						box-shadow 0 4px 2px rgba(#684e00, .42)
		&__loadmore
			.btn
				width auto
				padding 0 55px
	.faq
		&__item
			&--active
				& ^[1]-question
					&:before
						background-image url('data:image/png;base64,')
						transform translateY(-50%) rotate(180deg)
			&-question
				position relative
				padding 15px 40px 15px 45px
				&:before
					position absolute
					top 50%
					right 12px
					width 17px
					height 13px
					background-image url('data:image/png;base64,')
					content ''
					transform translateY(-50%)
					transition all .2s
	.pricing
		&__nav
			padding 50px 0 60px
			&:before
				z-index 3
		&__scrollto
			width auto
			text-align center
			&-item
				display inline-block
				width 158px
				height 49px
				line-height 45px
				margin 0 15px
		&__section
			&--day
				& ^[1]-title
					&:after
						background-color #303030
			&--week
				& ^[1]-title
					&:after
						background-color #2a2a2a
			&--month
				& ^[1]-title
					&:after
						background-color #242424
			&-title
				position relative
				overflow hidden
				margin 0 0 -15px
				&:after
					position absolute
					top 0
					right 0
					bottom 0
					width 64px
					content ''
				span
					position relative
					&:before
					&:after
						position absolute
						top 50%
						width calc(1000% - 30px)
						height 9px
						border solid #3b3b3b
						border-width 2px 0
						content ''
						transform translateY(-50%)
					&:before
						left -1000%
					&:after
						right -1000%
		&__list
			.flickity-prev-next-button
				top -76px
/*  </768px>  */